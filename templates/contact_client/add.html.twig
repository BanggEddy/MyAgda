{% extends 'client.html.twig' %}

{% block body %}
    <h1>Ajouter un contact</h1>

    {{ form_errors(form) }}

    {{ form_start(form) }}

    {{ form_row(form.nom) }}
    {{ form_row(form.prenom) }}
    {{ form_row(form.email) }}
    {{ form_row(form.telephone) }}

    {{ form_row(form._token) }}

    <button type="submit" class="btn btn-success">Ajouter</button>

    {{ form_end(form) }}
{% endblock %}
